@{
    ViewData["Title"] = "Home Page";
}

<!-- Container -->
<div class="container py-4">
    <div class="row">
        <div class="col-lg-4 col-xl-3">
            <!-- Toggle Navigation Button -->
            <button type="button" class="btn btn-secondary btn-block d-lg-none mb-4" onclick="$('#navigation').toggleClass('d-none');">
                <i class="fa fa-fw fa-bars mr-1"></i> Navigation
            </button>

            <!-- Navigation -->
            <div id="navigation" class="block d-none d-lg-block mr-lg-4">
                <div class="block-content">
                    <nav class="nav nav-pills flex-column mb-0">
                        <div class="font-size-sm text-uppercase text-black-50 font-weight-bold mb-3">
                            Main
                        </div>
                        <a class="nav-link mb-2 active" href="">
                            <i class="fa fa-fw fa-chart-line mr-1"></i>
                            Dashboard
                        </a>

                        <a class="nav-link mb-2" asp-area="" asp-controller="Admin" asp-action="ListUsers">
                            <i class="fa fa-fw fa-school mr-1"></i>
                            Administration
                        </a>
                        <a class="nav-link mb-2" asp-area="" asp-controller="Admin" asp-action="ListRoles">
                            <i class="fa fa-fw fa-project-diagram mr-1"></i>
                            Roles
                        </a>
                        <a class="nav-link mb-2" href="">
                            <i class="fa fa-fw fa-user-friends mr-1"></i>
                            Customers
                        </a>
                        <a class="nav-link mb-2" asp-area="" asp-controller="Subject" asp-action="Index">
                            <i class="fa fa-book fa-book mr-1"></i>
                            Add Subjects
                        </a>
                        <a class="nav-link mb-2" asp-area="" asp-controller="SchoolType" asp-action="Index">
                            <i class="fa fa-fw fa-school mr-1"></i>
                            School Type
                        </a>
                        <a class="nav-link mb-2" asp-area="" asp-controller="Specialization" asp-action="Index">
                            <i class="fa fa-fw fa-pencil-alt mr-1"></i>
                            Specialization
                        </a>
                        <a class="nav-link mb-2" asp-area="" asp-controller="GradesTaught" asp-action="Index">
                            <i class="fa fa-fw fa-pencil-alt mr-1"></i>
                            Grade Taught
                        </a>
                        <a class="nav-link mb-2" asp-area="" asp-controller="Interest" asp-action="Index">
                            <i class="fa fa-fw fas fa-thumbs-up mr-1"></i>
                            Interest
                        </a>
                        <div class="font-size-sm text-uppercase text-black-50 font-weight-bold mb-3 mt-4">
                            Account
                        </div>
                        <a class="nav-link mb-2" href="">
                            <i class="fa fa-fw fa-inbox mr-1"></i>
                            Inbox
                        </a>
                        <a class="nav-link mb-2" href="">
