@section Scripts {




    <!-- Custom JS -->
    <script>
        jQuery(function () {
            // Set Global Chart.js configuration
            Chart.defaults.global.defaultFontColor = 'rgba(0,0,0,.75)';
            Chart.defaults.scale.gridLines.color = 'transparent';
            Chart.defaults.scale.gridLines.zeroLineColor = 'transparent';
            Chart.defaults.global.elements.line.borderWidth = 2;
            Chart.defaults.global.elements.point.radius = 6;
            Chart.defaults.global.elements.point.hoverRadius = 8;
            Chart.defaults.global.tooltips.cornerRadius = 4;
            Chart.defaults.global.legend.display = false;

            // Get Chart Container
            var chartVisitors = jQuery('.js-chartjs-visitors');

            // Init Chart
            var chart = new Chart(chartVisitors, {
                type: 'line',
                data: {
                    labels: ["Mon", "Tue", "Wed", "Thu", "Fri", "Sat", "Sun"],
                    datasets: [
                        {
                            label: 'Visitors',
                            fill: true,
                            backgroundColor: 'rgba(46, 204, 113, .25)',
                            borderColor: 'rgba(46, 204, 113, 1)',
                            pointBackgroundColor: 'rgba(46, 204, 113, 1)',
                            pointBorderColor: '#fff',
                            pointHoverBackgroundColor: '#fff',
                            pointHoverBorderColor: 'rgba(46, 204, 113, 1)',
                            data: [12234, 15532, 28933, 19132, 23432, 26679, 36789]
                        }
                    ]
                },
                options: {
                    maintainAspectRatio: false,
                    scales: {
                        yAxes: [{
                            ticks: {
                                suggestedMin: 0,
                                suggestedMax: 40000
                            }
                        }]
                    },
                    tooltips: {
                        intersect: false,
                        callbacks: {
                            label: function (tooltipItems, data) {
                                return ' ' + tooltipItems.yLabel + ' Page views';
                            }
                        }
                    }
                }
            });
        });
    </script>
    <partial name="_ValidationScriptsPartial" />
}